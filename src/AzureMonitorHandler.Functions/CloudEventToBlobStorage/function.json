{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "blob",
      "name": "outputBlob",
      "path": "events/{rand-guid}.json",
      "connection": "OUTPUT_STORAGE_ACCOUNT",
      "direction": "out"
    }
  ]
}
